#include <stdio.h>

int main() {
    int num1, num2, num3;
    int biggest, smallest;

    printf("Please enter a number:\n");
    scanf("%d", &num1);

    printf("Please enter another number:\n");
    scanf("%d", &num2);

    printf("Please enter a third number:\n");
    scanf("%d", &num3);

    biggest = num1;
    smallest = num1;

    if (num2 > biggest) {
        biggest = num2;
    }
    if (num2 < smallest) {
        smallest = num2;
    }

    if (num3 > biggest) {
        biggest = num3;
    }
    if (num3 < smallest) {
        smallest = num3;
    }

    printf("\nBiggest number was: %d\n", biggest);
    printf("Smallest number was: %d\n", smallest);

    return 0;
}
